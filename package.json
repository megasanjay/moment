{
  "name": "moment",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn setup && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 .",
    "typecheck": "tsc --noEmit --incremental false",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "setup": "NODE_OPTIONS='--experimental-json-modules' node ./scripts/setup.mjs",
    "postbuild": "next-sitemap",
    "prepare": "run-script-os",
    "prepare:win32": "husky install",
    "prepare:darwin:linux": "husky install && chmod ug+x .husky/*  && chmod ug+x .git/hooks/*"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint . --fix",
      "prettier -w"
    ],
    "./**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^2.0.0",
    "@next-auth/mongodb-adapter": "^1.0.3",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.11",
    "dotenv": "^16.0.1",
    "js-cookie": "^3.0.1",
    "mongodb": "^4.6.0",
    "next": "13.2.3",
    "next-auth": "^4.3.1",
    "next-mdx-remote": "^4.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-textarea-autosize": "^8.3.4",
    "remark": "^14.0.2",
    "remark-mdx": "^2.1.1",
    "swr": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@fairdataihub/config": "^2.2.1",
    "@faker-js/faker": "^7.1.0",
    "@types/js-cookie": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "megasanjay-devmoji": "^1.2.1",
    "next-sitemap": "^4.0.2",
    "postcss": "^8.4.12",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^3.0.23",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^4.6.2"
  }
}
